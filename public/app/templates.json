    {
        "mime_type": "test_manager/inspector",
        "name": "ipv4_packet_flooder",
        "inputs": {
            "packet_size": {
                "type": "select",
                "options": [
                    "",
                    "small",
                    "large",
                    "custom"
                ],
                "label": "Packet Size",
                "group": "general",
                "index": 1,
                "attrs": {
                    "label": {
                        "data-tooltip": "packet size"
                    }
                }
            },
            "payload_size": {
                "type": "text",
                "label": "Custom Payload Size",
                "group": "general",
                "index": 2,
                "attrs": {
                    "label": {
                        "data-tooltip": "custom payload size"
                    }
                },
                "when": {
                    "eq": {
                        "packet_size": "custom"
                    }
                }
            },
            "ttl": {
                "type": "text",
                "label": "TTL",
                "group": "general",
                "index": 3,
                "defaultValue": 64,
                "attrs": {
                    "label": {
                        "data-tooltip": "time to live"
                    }
                }
            },
            "protocol": {
                "type": "select",
                "label": "Protocol",
                "options": [
                    "",
                    "ICMP",
                    "UDP",
                    "NTP (Reflection)",
                    "IGMP",
                    "BGP",
                    "RIP"
                ],
                "group": "protocol",
                "index": 1,
                "attrs": {
                    "select": {
                        "data-placeholder": "Choose protocol"
                    }
                }
            },
            "icmp_type": {
                "type": "select",
                "label": "ICMP Type",
                "options": [
                    "Type 0 — Echo Reply",
                    "Type 1 — Unassigned",
                    "Type 2 — Unassigned",
                    "Type 3 — Destination Unreachable",
                    "Type 4 — Source Quench (Deprecated)",
                    "Type 5 — Redirect",
                    "Type 6 — Alternate Host Address (Deprecated)",
                    "Type 7 — Unassigned",
                    "Type 8 — Echo",
                    "Type 9 — Router Advertisement",
                    "Type 10 — Router Selection",
                    "Type 11 — Time Exceeded",
                    "Type 12 — Parameter Problem",
                    "Type 13 — Timestamp",
                    "Type 14 — Timestamp Reply",
                    "Type 15 — Information Request (Deprecated)",
                    "Type 16 — Information Reply (Deprecated)",
                    "Type 17 — Address Mask Request (Deprecated)",
                    "Type 18 — Address Mask Reply (Deprecated)",
                    "Type 19 — Reserved (for Security)",
                    "Types 20-29 — Reserved (for Robustness Experiment)",
                    "Type 30 — Traceroute (Deprecated)",
                    "Type 31 — Datagram Conversion Error (Deprecated)",
                    "Type 32 — Mobile Host Redirect (Deprecated)",
                    "Type 33 — IPv6 Where-Are-You (Deprecated)",
                    "Type 34 — IPv6 I-Am-Here (Deprecated)",
                    "Type 35 — Mobile Registration Request (Deprecated)",
                    "Type 36 — Mobile Registration Reply (Deprecated)",
                    "Types 37 — Domain Name Request (Deprecated)",
                    "Types 38 — Domain Name Reply (Deprecated)",
                    "Type 39 — SKIP (Deprecated)",
                    "Type 40 — Photuris"
                ],
                "group": "protocol",
                "index": 2,
                "when": {
                    "eq": {
                        "protocol": "ICMP"
                    }
                }
            },
            "icmp_code": {
                "type": "select",
                "label": "ICMP Code",
                "options": [
                    "No code"
                ],
                "group": "protocol",
                "index": 3,
                "when": {
                    "eq": {
                        "protocol": "ICMP"
                    }
                }
            },
            "randomize_src_port": {
                "type": "toggle",
                "label": "Randomize Source Port",
                "group": "protocol",
                "index": 2,
                "when": {
                    "eq": {
                        "protocol": "UDP"
                    }
                }
            },
            "randomize_dst_port": {
                "type": "toggle",
                "label": "Randomize Destinatio Port",
                "group": "protocol",
                "index": 3,
                "when": {
                    "eq": {
                        "protocol": "UDP"
                    }
                }
            },
            "fixed_port": {
                "type": "text",
                "label": "Fixed Port",
                "group": "protocol",
                "attrs": {
                    "label": {
                        "data-tooltip": "fixed port (0-65535)"
                    }
                },
                "index": 4,
                "when": {
                    "eq": {
                        "protocol": "UDP"
                    }
                }
            },
            "tcp_flood": {
                "type": "select",
                "label": "TCP Flood",
                "options": [
                    "SYN",
                    "ACK",
                    "PSH",
                    "RST",
                    "FIN",
                    "URG"
                ],
                "group": "tcp_flood",
                "index": 1
            },
            "mtu": {
                "type": "text",
                "label": "MTU",
                "group": "fragment_packets",
                "attrs": {
                    "label": {
                        "data-tooltip": "maximum transmission unit (MTU) size"
                    }
                }
            },
            "always_set_more_fragments": {
                "type": "toggle",
                "label": "Always Set More Fragments",
                "group": "fragment_packets"
            }
        },
        "groups": {
            "general": {
                "label": "General",
                "index": 1
            },
            "protocol": {
                "label": "Protocol",
                "index": 2
            },
            "tcp_flood": {
                "label": "TCP Flood",
                "index": 3,
                "closed": true
            },
            "fragment_packets": {
                "label": "Fragment Packets",
                "index": 4,
                "closed": true
            }
        }
    },
    {
        "mime_type": "test_manager/inspector",
