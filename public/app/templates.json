                        "data-tooltip": "reflection"
                    }
                }
            },
            "other": {
                "type": "text",
                "label": "Other",
                "group": "general",
                "index": 4,
                "attrs": {
                    "label": {
                        "data-tooltip": "other"
                    }
                }
            },
            "tcp_hanging_connection": {
                "type": "toggle",
                "label": "Hanging connection",
                "group": "general",
                "index": 5,
                "attrs": {
                    "label": {
                        "data-tooltip": "TCP hanging connection"
                    }
                }
            }
        },
        "groups": {
            "general": {
                "label": "General",
                "index": 1
            }
        }
    },
    {
        "mime_type": "test_manager/inspector",
        "name": "http",
        "inputs": {
            "target_port": {
                "type": "text",
                "label": "Target port",
                "group": "header",
                "index": 1
            },
            "method": {
                "type": "select",
                "options": [
                    "",
                    "GET",
                    "HEAD",
                    "POST",
                    "PUT",
                    "DELETE",
                    "TRACE",
                    "CONNECT",
                    "OPTIONS",
                    "PROPFIND",
                    "PROPPATCH",
                    "MKCOL",
                    "COPY",
                    "MOVE",
                    "LOCK",
                    "UNLOCK",
                    "VERSION-CONTROL",
                    "REPORT",
                    "CHECKOUT",
                    "CHECKIN",
                    "UNCHECKOUT",
                    "MKWORKSPACE",
                    "UPDATE",
                    "LABEL",
                    "MERGE",
                    "BASELINE-CONTROL",
                    "MKACTIVITY",
                    "ORDERPATCH",
                    "ACL",
                    "PATCH",
                    "SEARCH",
                    "RFC 2518 ---------",
                    "RFC 2616 ---------",
                    "RFC 3253 ---------",
                    "RFC 3648 ---------",
                    "RFC 3744 ---------",
                    "draft-dusseault-http-patch ---------",
                    "draft-reschke-webdav-search ---------"
                ],
                "label": "Method",
                "group": "header",
                "index": 2,
                "attrs": {
                    "select": {
                        "data-placeholder": "Choose method"
                    }
                }
            },
            "thread_concurrency": {
                "type": "text",
                "label": "Thread concurrency",
                "group": "connection_rates",
                "index": 1,
                "attrs": {
                    "label": {
                        "data-tooltip": "Concurrent request threads (1-2000) for HTTP Floods"
                    }
                }
            },
            "keep_alive": {
                "type": "toggle",
                "label": "Keep-alive",
                "group": "connection_rates",
                "index": 2,
                "attrs": {
                    "label": {
                        "data-tooltip": "Keep-alive to re-use connection for multiple requests"
                    }
                }
            },
            "random_user_agent": {
                "type": "toggle",
                "label": "Random user agent",
                "group": "randomization",
                "index": 1,
                "attrs": {
                    "label": {
                        "data-tooltip": "Randomize user agent from built-in list of 600+ user agents"
                    }
                }
            },
            "random_data_get": {
                "type": "toggle",
                "label": "Random data",
                "group": "randomization",
                "index": 2,
                "attrs": {
                    "label": {
                        "data-tooltip": "Add random key=value to every GET request on URL to avoid caching"
                    }
                }
            },
            "true_client_ip_headers": {
                "type": "toggle",
                "label": "True client IP",
                "group": "vector_options",
                "index": 1,
                "attrs": {
                    "label": {
                        "data-tooltip": "Add Akamai-style true-client-ip header"
                    }
                }
            },
            "x_forwarded_for_headers": {
                "type": "toggle",
                "label": "X-forwarder for headers",
                "group": "vector_options",
                "index": 2,
                "attrs": {
                    "label": {
                        "data-tooltip": "Add Proxy/Load Balance style x-forwarded-for header"
                    }
                }
            },
            "http_flood_direct": {
                "type": "toggle",
                "label": "Regular request flood",
                "group": "engines",
                "index": 1,
                "attrs": {
                    "label": {
                        "data-tooltip": "each request waits for a response"
                    }
                }
            },
            "hit_and_run_direct": {
                "type": "toggle",
                "label": "Hit and Run Flood",
                "group": "engines",
                "index": 2,
                "attrs": {
                    "label": {
                        "data-tooltip": "client termiantes connection right after request, before reply received"
                    }
                }
            },
            "slowloris_direct": {
                "type": "toggle",
                "label": "Slowloris",
                "group": "engines",
                "index": 3,
                "attrs": {
                    "label": {
                        "data-tooltip": "Requests sent will never complete"
                    }
                }
            },
            "slow_read_direct": {
                "type": "toggle",
                "label": "Slow Read",
                "group": "engines",
                "index": 4,
                "attrs": {
                    "label": {
                        "data-tooltip": "Request sent, response read slowly"
                    }
                }
            },
            "slow_write_direct": {
                "type": "toggle",
                "label": "Slow Write",
                "group": "engines",
                "index": 5,
                "attrs": {
                    "label": {
                        "data-tooltip": "Data sent to server slowly -- good with posts"
                    }
                }
            },
            "bad_data": {
                "type": "toggle",
                "label": "Bad Data",
                "group": "engines",
                "index": 6,
                "attrs": {
                    "label": {
                        "data-tooltip": "Random data is sent to the server"
                    }
                }
            },
            "headless_browser_direct": {
                "type": "toggle",
                "label": "Headless browser emulation",
                "group": "engines",
                "index": 7,
                "attrs": {
                    "label": {
                        "data-tooltip": "a 'webkit' browser flood (+5 to +100 requests/sec per agent)"
                    }
                }
            },
            "tor_requests_enable": {
                "type": "toggle",
                "label": "Enable TOR",
                "group": "tor",
                "index": 1,
                "attrs": {
                    "label": {
                        "data-tooltip": "enable the Onion Router"
                    }
                }
            },
            "http_flood_tor": {
                "type": "toggle",
                "label": "Reqular request flood",
                "group": "tor",
                "index": 2,
                "attrs": {
                    "label": {
                        "data-tooltip": "each request waits for a response (slower than direct)"
                    }
                },
                "when": {
                    "eq": {
                        "tor_requests_enable": true
                    }
                }
            },
            "slow_read_tor": {
                "type": "toggle",
                "label": "TOR: Slow Read",
                "group": "tor",
                "index": 3,
                "attrs": {
                    "label": {
                        "data-tooltip": "incompatible with some options"
                    }
                },
                "when": {
                    "eq": {
                        "tor_requests_enable": true
                    }
                }
            },
            "slow_write_tor": {
                "type": "toggle",
                "label": "Slow Write",
                "group": "tor",
                "index": 4,
                "attrs": {
                    "label": {
                        "data-tooltip": "incompatible with some options"
                    }
                },
                "when": {
                    "eq": {
                        "tor_requests_enable": true
                    }
                }
            },
            "bad_data_tor": {
                "type": "toggle",
                "label": "Bad Data",
                "group": "tor",
                "index": 5,
                "attrs": {
                    "label": {
                        "data-tooltip": "random data is sent to the server"
                    }
                },
                "when": {
                    "eq": {
                        "tor_requests_enable": true
                    }
                }
            },
            "headless_browser_tor": {
                "type": "toggle",
                "label": "Headless browser emulation",
                "group": "tor",
                "index": 6,
                "attrs": {
                    "label": {
                        "data-tooltip": "a 'webkit' browser flood (+5 to +100 requests/sec per agent)"
                    }
                },
                "when": {
                    "eq": {
                        "tor_requests_enable": true
                    }
                }
            }
        },
        "groups": {
            "header": {
                "label": "Override HTTP Host Header",
                "index": 1
            },
            "connection_rates": {
                "label": "Connection Rates",
                "index": 2
            },
            "randomization": {
                "label": "Randomization",
                "index": 3,
                "closed": true
            },
            "vector_options": {
                "label": "HTTP Attack Vector Option",
                "index": 4,
                "closed": true
            },
            "engines": {
                "label": "Engines",
                "index": 5,
                "closed": true
            },
            "tor": {
                "label": "The Onion Router",
                "index": 6,
                "closed": true
            }
        }
    },
